{
  "manifest_version": 3,
  "name": "Chess Game Analyzer",
  "version": "1.0",
  "description": "Analyze chess.com games with AI",
  "permissions": [],
  "host_permissions": [
    "https://api.chess.com/*",
    "https://chessmover-625329947111.us-central1.run.app/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
} 